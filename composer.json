{
    "name": "albertohippo/bimdataclient",
    "description": "Component for working with bimdata.io",
    "keywords": ["BIMdata"],
    "type": "library",
    "version": "0.1",
    "homepage": "https://bitbucket.org/albertohippo/bimdataclient",
    "license": "MIT",
    "authors": [
        {
            "name": "AndreyKurmel",
            "email": "kurmeldeveloper@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",

    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    }
}
